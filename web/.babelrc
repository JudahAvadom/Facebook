{
    "presets": [
        [
            "@babel/preset-env", 
            {
                "useBuiltIns": "entry",
                "corejs": 3
            }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        ["@babel/plugin-syntax-jsx"],
        [
            "@babel/plugin-transform-runtime",
            {
                "regenerator": true
            }
        ]
    ]
}